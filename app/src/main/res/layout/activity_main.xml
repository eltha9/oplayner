<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="TextView"
            android:layout_width="227dp"
            android:layout_height="97dp"
            tools:layout_editor_absoluteX="105dp" android:id="@+id/textView"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/button"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="185dp" android:id="@+id/textView2"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/textView"/>
    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                                android:id="@+id/guideline" app:layout_constraintGuide_begin="20dp"
                                                android:orientation="vertical"/>
    <ImageView
            android:layout_width="382dp"
            android:layout_height="241dp" app:srcCompat="@drawable/ic_undraw_home_llustration"
            tools:layout_editor_absoluteX="15dp" android:id="@+id/imageView3"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView4"/>
    <ImageView
            android:layout_width="162dp"
            android:layout_height="86dp" app:srcCompat="@drawable/logo" tools:layout_editor_absoluteY="34dp"
            tools:layout_editor_absoluteX="124dp" android:id="@+id/imageView4"/>
</androidx.constraintlayout.widget.ConstraintLayout>